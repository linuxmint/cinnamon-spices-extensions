{
    "animation-duration": {
		"type": "spinbutton",
		"default" : 500,
		"min" : 100,
		"max" : 1000,
		"step" : 50,
		"units" : "milliseconds",
		"description" : "Animation Duration:",
		"tooltip" : ""
	},
    "animation-effect": {
        "type": "tween",
        "description": "Animation Easing:",
        "default": "easeNone"
    },
    "slip-factor": {
        "type": "scale",
        "description": "Slip Factor:",
        "min": -0.20,
        "max": 2,
        "step": 0.01,
        "default": 0,
        "tooltip" : ""
    },
    "sliding-background": {
		"type": "checkbox",
		"default" : true,
		"description" : "Sliding Background",
		"tooltip" : ""
	},
    "zoom-level": {
        "type": "scale",
        "description": "Background Zoom Level:",
        "min": 1.05,
        "max": 4,
        "step": 0.05,
        "default": 1.5,
        "dependency": "sliding-background",
        "indent": true,
        "tooltip" : ""
    },
    "vertical-position": {
        "type": "scale",
        "description": "Background Vertical Position:",
        "min": 0,
        "max": 1,
        "step": 0.01,
        "default": 0.5,
        "dependency": "sliding-background",
        "indent": true,
        "tooltip" : ""
    }
}
