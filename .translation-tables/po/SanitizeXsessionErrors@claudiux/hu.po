#. metadata.json->name
#: 4.8/extension.js:72
msgid "Sanitize ~/.xsession-errors"
msgstr "~/.xsession-errors megtisztítása"

#: 4.8/extension.js:73
msgid ""
"This extension has been disabled because it was no longer needed since "
"Cinnamon 4.8."
msgstr ""
"Ezt a kiterjesztést letiltották, mert a Cinnamon 4.8 óta már nem volt rá "
"szükség."

#. metadata.json->description
msgid ""
"Filter messages sent to ~/.xsession-errors, to avoid flooding and save your "
"SSD."
msgstr ""
"Az ~/.xsession-errors számára küldött üzenetek szűrése, hogy elkerülje a "
"felesleges adatáradatot és védje SSD-jét."

#. settings-schema.json->section0->description
msgid "Filter \"Log level 128\" messages starting with these words"
msgstr ""
"A „Log level 128” üzenetek szűrése, amelyek a következő szavakkal kezdődnek"

#. settings-schema.json->forbidden_words->description
msgid "Word list"
msgstr "Szólista"

#. settings-schema.json->forbidden_words->columns->title
msgid "Words"
msgstr "Szó"

#. settings-schema.json->forbidden_words->columns->title
msgid "To filter?"
msgstr "Szűrendő"

#. settings-schema.json->section1->description
msgid "~/.xsession-errors monitor"
msgstr "~/.xsession-errors figyelő"

#. settings-schema.json->open_log->description
msgid "Show the contents of ~/.xsession-errors in real time"
msgstr "~/.xsession-errors tartalmának valós idejű megjelenítése"
