#. metadata.json->name
msgid "Centered Cinnamon Dock"
msgstr "Dock Centralizado do Cinnamon"

#. metadata.json->description
msgid "Centers the panel with dock styling and customization options"
msgstr "Centraliza o painel com estilo de dock e opções de personalização"

#. 6.4->settings-schema.json->transparency->units
msgid "percent"
msgstr "porcentagem"

#. 6.4->settings-schema.json->transparency->description
msgid "Transparency"
msgstr "Transparência"

#. 6.4->settings-schema.json->height-offset->units
#. 6.4->settings-schema.json->hover-pixels->units
msgid "pixels"
msgstr "pixels"

#. 6.4->settings-schema.json->height-offset->description
msgid "Height offset (negative = up, positive = down)"
msgstr "Deslocamento de altura (negativo = cima, positivo = baixo)"

#. 6.4->settings-schema.json->hover-pixels->description
msgid "Hover trigger zone height at bottom of screen"
msgstr "Altura da zona de ativação na parte inferior da tela"

#. 6.4->settings-schema.json->animation-time->units
msgid "milliseconds"
msgstr "milissegundos"

#. 6.4->settings-schema.json->animation-time->description
msgid "Fade animation duration"
msgstr "Duração da animação de esmaecimento"

#. 6.4->settings-schema.json->auto-hide->description
msgid "Auto-hide dock when focusing apps"
msgstr "Ocultar automaticamente o dock ao focar aplicativos"

#. 6.4->settings-schema.json->no-window-shift->description
msgid "Don't shift windows (disable panel struts)"
msgstr "Não deslocar janelas (desativar suportes do painel)"

#. 6.4->settings-schema.json->show-on-no-focus->description
msgid "Show dock when no window is focused"
msgstr "Mostrar dock quando nenhuma janela está focada"