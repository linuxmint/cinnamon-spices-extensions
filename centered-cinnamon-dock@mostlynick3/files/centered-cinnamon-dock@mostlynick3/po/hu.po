#. metadata.json->name
msgid "Centered Cinnamon Dock"
msgstr "Középső Cinnamon Dokk"

#. metadata.json->description
msgid "Centers the panel with dock styling and customization options"
msgstr ""
"Középre igazítja a panelt a dokk stílus és a testreszabási "
"lehetőségek segítségével"

#. 6.4->settings-schema.json->transparency->units
msgid "percent"
msgstr "százalék"

#. 6.4->settings-schema.json->transparency->description
msgid "Transparency"
msgstr "Átláthatóság"

#. 6.4->settings-schema.json->height-offset->units
#. 6.4->settings-schema.json->hover-pixels->units
msgid "pixels"
msgstr "képpont"

#. 6.4->settings-schema.json->height-offset->description
msgid "Height offset (negative = up, positive = down)"
msgstr "Magasság eltolás (negatív = felfelé, pozitív = lefelé)"

#. 6.4->settings-schema.json->hover-pixels->description
msgid "Height of trigger zone to show panel if hidden"
msgstr ""
"A triggerzónának a magassága, amelyen a panel rejtett állapotban "
"látható"

#. 6.4->settings-schema.json->animation-time->units
msgid "milliseconds"
msgstr "ezredmásodperc"

#. 6.4->settings-schema.json->animation-time->description
msgid "Fade animation duration"
msgstr "Elhalványuló animáció időtartama"

#. 6.4->settings-schema.json->auto-hide->description
msgid "Auto-hide dock when focusing apps"
msgstr "Dokk automatikus elrejtése alkalmazások fókuszálásakor"

#. 6.4->settings-schema.json->no-window-shift->description
msgid "Don't shift windows (disable panel struts)"
msgstr ""
"Ne mozdítsa el az ablakokat (kapcsolja ki a panelmerevítőket)"

#. 6.4->settings-schema.json->show-on-no-focus->description
msgid "Show dock when no window is focused"
msgstr "Dokk megjelenítése, ha egyetlen ablak sincs fókuszban"

#. 6.4->settings-schema.json->panel-mode->options
msgid "Main panel only"
msgstr "Csak a fő panel"

#. 6.4->settings-schema.json->panel-mode->options
msgid "All bottom panels"
msgstr "Minden alsó panel"

#. 6.4->settings-schema.json->panel-mode->options
msgid "All top panels"
msgstr "Minden felső panel"

#. 6.4->settings-schema.json->panel-mode->options
msgid "Top and bottom panels"
msgstr "Felső és alsó panelek"

#. 6.4->settings-schema.json->panel-mode->description
msgid "Which panels to apply extension to"
msgstr "Mely panelekre kell kiterjesztést alkalmazni"
